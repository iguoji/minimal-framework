{
    "name": "iguoji/minimal",
    "type": "library",
    "description": "a minimalism swoole framework",
    "keywords": ["framework", "swoole", "php"],
    "homepage": "https://github.com/iguoji/minimal",
    "require": {
        "php": ">=8.0.0",
        "iguoji/minimal-annotation": "dev-master",
        "iguoji/minimal-validate": "dev-master",
        "iguoji/minimal-container": "dev-master",
        "iguoji/minimal-support": "dev-master",
        "iguoji/minimal-database": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Minimal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "tests/app/"
        }
    },
    "minimum-stability": "dev"
}