{
    "name": "iguoji/minimal-framework",
    "type": "library",
    "description": "a minimalism framework",
    "keywords": ["framework", "php"],
    "homepage": "https://github.com/iguoji/minimal-framework",
    "license": "Apache-2.0",
    "require": {
        "php": "^8.0",
        "ext-mbstring": "*"
    },
    "autoload": {
        "psr-4": {
            "Minimal\\": "src/",
            "App\\": "tests/app/"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
